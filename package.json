{
  "name": "marcopaps-contentful-nextjs",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm codegen && next build",
    "start": "next build && next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "codegen": "contentful-typescript-codegen --output types/generated/contentful.d.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf ./node_modules",
    "check-outdated": "npx --yes -- check-outdated --ignore-pre-releases --ignore-dev-dependencies --ignore-packages package1,package2 --columns name,type,current,latest,changes --types major,minor,patch,reverted",
    "preversion": "npm run lint && npm run test && npm run check-outdated"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.19.0",
    "axios": "1.6.0",
    "contentful": "10.6.5",
    "dayjs": "1.11.10",
    "dotenv": "16.3.1",
    "next": "14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@next/bundle-analyzer": "14.0.1",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.8.10",
    "@types/react": "18.2.33",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "contentful-management": "11.3.0",
    "contentful-typescript-codegen": "3.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "rimraf": "5.0.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  }
}
